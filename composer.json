{
  "name": "generative/admin-database-provider",
  "description": "A module for providing access to tables for the admin panel",
  "type": "module",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Kornienko",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "AdminDatabaseProvider\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0|^10.0"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan vendor:publish --tag=admin-panel-config --force"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdminDatabaseProvider\\Providers\\AdminDatabaseProvider"
      ],
      "files": [
        "config/admin_panel.php"
      ]
    }
  }
}
